/* pages/index/search/search.wxss */
.search-title{
    display: flex;
    padding: 20rpx;
    align-items: center;
}
.search-bar{
    display: flex;
    position: relative;
    transform:translateX(-40rpx);
    border:2rpx solid #e8e8ec;
}
.search{
    position: absolute;
    z-index: 2;
    left:16rpx;
    top:16rpx;
}
.cancel-icon{
    position: absolute;
    right:30rpx;
    z-index:2;
    top:16rpx;
}
.search-bar>input{
    width:408rpx;
    height:64rpx;
    border-radius:0 10rpx 10rpx 0;
    background-color:#fff;
    padding-left: 60rpx;
}
.placeholder{
    font-size:26rpx;
    color: #d6d6d9;
}

.phone_personal{
    color:#666;
    font-size:26rpx;
    text-align: center;
}
.phone_personal:before{
    content: '';
    width: 14rpx;
    height: 14rpx;
    border-bottom: 1px solid #666;
    border-right:1px solid #666;
    position: absolute;
    top:18rpx;
    right:14rpx;
    transform:rotate(45deg);

}
.phone_one{
    display: flex;
    position: relative;
    justify-content:flex-start;
    align-items: center;
    background-color:#fff;
    width:150rpx;
    height:56rpx;
    line-height: 56rpx;
    margin:0 auto;
    border-radius: 10rpx 0 0 10rpx;
    border:2rpx solid #e8e8ec;
    padding: 5rpx;
}
.phone_one>image{
    width:40rpx;
    height:40rpx;
    margin-right: 10rpx;
}
.person_box{
    position: relative;
}
.phone_select{
    margin-top:0;
    z-index: 100;
    position: absolute;
    width:148rpx;
    left:-198rpx;
    top:35rpx;
    padding: 5rpx;
    background: #fff;
    border:1rpx solid #eee;
}
.select_one{
    text-align: center;
    background-color:#fff;
    height:56rpx;
    line-height:56rpx;
    padding:0 5%;
    border-bottom:1rpx solid #eee;
}
.select_one:last-child{
    border-bottom: none;
}
.history{
    background: #fff;
}
.history>view{
    height:88rpx;
    line-height: 88rpx;
    font-size: 26rpx;
    color:#666;
    padding-left: 60rpx;
    border-bottom: 1rpx solid #eee;
}
.clearAway{
    display: flex;
    align-items: center;
    justify-content: center;
    padding-top: 80rpx;
}
.clearAway>view{
    width:360rpx;
    height:70rpx;
    line-height: 70rpx;
    border:1rpx solid #ccc;
    color:#999;
    font-size: 32rpx;
    border-radius: 10rpx;
    text-align: center;
}
/*商品*/
.commodity{
    background: #fff;
    border-top: 1rpx solid #e5e5e5;
}
.commodity>.commodityFirst{
    display: flex;
    justify-content: center;
    border-bottom: 1rpx solid #e5e5e5;
}
.commodity .menuList{
    width:150rpx;
    text-align: center;
    color:#999;
    font-size: 26rpx;
    height:70rpx;
    line-height: 70rpx;
}
.commodity .menuList.active{
    color:#da444f;
    border-bottom: 4rpx solid #da444f;
}
.commodityList{
    display: flex;
    align-items: center;
    justify-content: flex-start;
    padding: 20rpx 30rpx;
    border-bottom: 1rpx solid #e5e5e5;
}
.commodityList>view:first-child>image{
    width:130rpx;
    height:130rpx;
    border-radius: 10rpx;
    border: 1rpx solid #e5e5e5;
    margin-right: 30rpx;
}
.commodityList>.lists>view{
    display:block;
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
    width:465rpx;
    margin-bottom: 10rpx;
}
.commodityList>.lists>view:first-child{
    color:#000;
    font-size: 36rpx;
}
.commodityList>.lists>view:nth-child(2){
    color:#aaa;
    font-size: 26rpx;
}
.commodityList>.lists>view:nth-child(3){
    color:#da444f;
    font-size: 28rpx;
}
.commodityList>.lastImg>image{
    width:50rpx;
    height:50rpx;
}
/*档口*/
.stalls{
    background: #fff;
    border-top: 1rpx solid #e5e5e5;
}
.stallsList{}
.stallsList{
    display: flex;
    align-items: center;
    justify-content: flex-start;
    padding: 20rpx 30rpx;
    border-bottom: 1rpx solid #e5e5e5;
}
.stallsList>view:first-child>image{
    width:130rpx;
    height:130rpx;
    border-radius: 10rpx;
    border: 1rpx solid #e5e5e5;
    margin-right: 30rpx;
}
.stallsList>.lists>view{
    display:block;
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
    width:300rpx;
    margin-bottom: 10rpx;
}
.stallsList>.lists>view:first-child{
    color:#000;
    font-size: 36rpx;
}
.stallsList>.lists>view:nth-child(2),.stallsList>.lists>view:nth-child(3){
    color:#aaa;
    font-size: 26rpx;
}
.stallsList>.lastImg>image{
    width:50rpx;
    height:50rpx;
    margin-left: 25rpx;
}