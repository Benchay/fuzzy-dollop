/* pages/user_info/my_order/my_order.wxss */
.my-container {
    width: 100%;
}
.message>view>.date{
    color:#b2b2b2;
    font-size: 24rpx;
    height:70rpx;
    line-height: 80rpx;
    text-align: center;
}
.message>view>.second{
    background: #fff;
    padding: 30rpx 0;
}
.second>view{
    line-height: 75rpx;
    padding:0 60rpx;
}
.second>view:first-child{
   color:#353535;
    font-size: 34rpx;
    font-weight: bold;
    position: relative;
}
.second>.system:after{
    content: '';
    width:10rpx;
    height:10rpx;
    border-radius: 50%;
    background: #da444f;
    position: absolute;
    left:30rpx;
    top:35rpx;
}
.second>view:last-child{
    color:#a9a9a9;
    font-size: 26rpx;
    line-height: 40rpx;
}