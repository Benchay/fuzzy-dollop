.my-container {
    width: 100%;
}
.user-wechat-info {
    width: 100%;
    height: 250rpx;
    background-color: #DA444F;
    overflow: hidden;
}
.order-details {
    width: 100%;
    height: 342rpx;
    background-color: white;
    margin-top: 19rpx;
}
.order-detail {
    width: 100%;
    height: 85rpx;
    background-color: white;
}
.order-detail:not(:last-child) {
    border-bottom: 1rpx solid #e9e8ed;
}
.order-detail:last-child {
    border-bottom: 1rpx solid #e9e8ed;
}
.discount-coupon {
    width: 100%;
    height: 85rpx;
    background-color: white;
    margin-top: 19rpx;
}
.detail_info {
    width: 682rpx;
    margin: 0 auto;
}
.user-wechat-info > image {
    width: 96rpx;
    height: 96rpx;
    margin: 30rpx 30rpx 42rpx -42rpx;
    border: 4rpx solid #BA4356;
    border-radius: 50%;
    /*display: block;*/
    display: inline-block;
    text-align: center;
    position: absolute; 
    left:50%
    
}

.words-info {
    float:left;
    height: 96rpx;
    width: 100%;
    margin: 130rpx 0;
    text-align: center;
}
.nick-name {
    color: white;
}
.tipinfo {
    color: #FFC7BC;
    font-size: 24rpx;
    margin-top: 22rpx;
}
.detail-info {
    height: 40rpx;
    padding: 22rpx 20rpx;
}
.order-icon {
    width: 39rpx;
    height: 41rpx;
    display: block;
    float: left;
    margin-right: 24rpx;
}
.detail-info > .item-name {
    display: block;
    float: left;
    line-height: 42rpx;
}
.right-array {
    width: 16rpx;
    height: 25rpx;
    float: right;
    margin-top: 10rpx;
}
.detail-info > .item-num {
    display: block;
    float: right;
    line-height: 42rpx;
    font-size: 24rpx;
    color:#c81624;
    margin-right: 28rpx;
}

