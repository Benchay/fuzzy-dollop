.my-container {
    border-top: 1rpx solid #e8e8e8;
    position: relative;
    min-height: 800rpx;
}

.cartElseImg {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 900rpx;
}

.errorwrap>image {
    width: 100%;
    height: 100%;
}

.errorwrap {
    width: 430rpx;
    height: 430rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -215rpx;
    margin-left: -215rpx;
}